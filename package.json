  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "npm run server:build && vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "node api/server.cjs",
    "server:build": "mkdir -p dist && cp -r api dist/api && cp -r scripts dist/scripts",
    "server:build:prod": "NODE_ENV=production npm run server:build",
    "server:start": "node dist/api/server.js",
    "server:prod": "NODE_ENV=production node api/dist/server.js",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "start": "npm run server:prod",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "mysql:migrate": "node scripts/migrate.cjs",
    "mysql:migrate:status": "node api/scripts/runMigrations.cjs --status",
    "mysql:test": "node api/scripts/testConnection.cjs",
    "mysql:setup": "node api/scripts/setupDatabase.cjs",
    "server:check": "node api/scripts/checkServer.cjs",
    "deploy": "node scripts/deploy.ps1",
    "backup": "node scripts/backup.cjs",
    "create-admin": "node scripts/create-admin.cjs",
    "monitor": "node scripts/monitor.cjs",
    "health-check": "node scripts/health-check.cjs",
    "maintenance": "node scripts/maintenance.cjs",
    "maintenance:quick": "node scripts/maintenance.cjs --no-db --no-report",
    "clean": "rm -rf dist api/dist node_modules/.vite",
    "install:prod": "npm ci --production",
    "postinstall": "npm run build"
  },
